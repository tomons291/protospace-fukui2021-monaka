<div class="card">
  <%= link_to image_tag(prototype.image, class: 'card__img'),  prototype_path(prototype.id) if prototype.image.attached? %>
  <div class="card__body">  
     <%= link_to prototype.id, prototype_path(prototype.id), class: :card__title%> 
     <p class="card__summary">  
      <%= prototype.catchcopy %>  
     </p>  
    <%# by<%= link_to prototype.user.name, user_path(user.id), class: :card__user %>   %>
  </div> 
</div>